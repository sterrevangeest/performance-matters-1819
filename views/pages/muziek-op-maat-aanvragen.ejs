<div class="music-lists">
<section class= "list">
  <h2>Opgeslagen</h2>
  <% result.forEach(function(item) { %>
    <div class="">
      <p><%= item.artist_name %></p>
      <p><%= item.track_name %></p>
    </div>
  <% }); %>
</section>

<section class="music-search">
  <% data.forEach(function(item) { %>
    <!-- <a href="muziek-op-maat-aanvragen/<%= item.track_id %>"> -->
      <div class="track">
          <div class="track-info">
            <img src="http://lorempixel.com/400/400" />
          <div class="">
            <p class="artist-name"><%= item.track_name %></p>
            <p><%= item.artist_name %></p>
            </div>
      </div>
      <button type="button" name="button" >+ toevoegen</button>
      </div>
